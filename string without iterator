#include <iostream> 
#include <map> 
#include <string> 
using namespace std; 
int main() 
{ 
    map<string, int> mp; 
    mp["one"] = 1; 
    mp["two"] = 2; 
    mp["three"] = 3; 
    mp["one"] = 4; 
     for (auto pair : mp) 
    { 
        cout << pair.first << " -> " << pair.second << endl; 
    } 
 cout<<""<<mp["one"]<<endl;
    return 0; 
}
